@{
    ViewBag.Title = "Data Tracking";
}


<head>

<script src="~/Scripts/knockout-3.4.2.js" type="text/javascript"></script>
<script src="~/Scripts/p5.js" type="text/javascript"></script>
<script src="~/Scripts/p5.dom.js" type="text/javascript"></script>
<script src="~/Scripts/p5.sound.js" type="text/javascript"></script>
<script src="~/Scripts/sketch.js" type="text/javascript"></script>

<script type="text/javascript">
    function twitterAlert() {
        //alert("In order to be using the most up to date tweet information run the bot.js node file before accessing this page.");
    }
</script>

</head>

<body>
<div class="jumbotron">
    <div class="jumboHead">
        <div class="jumboHeadPic"><img src="~/Content/Images/UADM_logo_noBackground.png" /></div> 
        <h1>@ViewBag.Title</h1>
    </div>
    <br /> <br /> <br />
    <div class="row">
        <div class="col-md-4">
            <div class="jumboContainer">
                <p>@Html.ActionLink("Twitter Tracker", "TwitterTracker", "Home", null, new { @class = "btn btn-primary btn-lg", onclick= "twitterAlert();" })</p>
                <p>Application that works with bot.js module to display recent UADM tweets. Allows the option to download the data to a CSV</p>
            </div>
        </div>
        <div class="col-md-4">
            <div class="jumboContainer">
                <p>@Html.ActionLink("Instagram Tracker", "InstagramTracker", "Home", null, new { @class = "btn btn-primary btn-lg" })</p>
                <p>Coming Soon!</p>
            </div>
        </div>
        <div class="col-md-4">
            <div class="jumboContainer">
                <p>@Html.ActionLink("Snapchat Tracker", "GPACalculator", "Home", null, new { @class = "btn btn-primary btn-lg" })</p>
                <p>Coming Soon!</p>
            </div>
        </div>
    </div>
    
    <br />
</div>
</body>


<div class="row">
    <div class="col-md-3">
        <p>@Html.ActionLink("Log In", "Login", "Account", null, new { @class = "btn btn-default" })</p>
        <p>Access your existing account</p>
    </div>
    <div class="col-md-3">
        <p>@Html.ActionLink("Register", "Register", "Account", null, new { @class = "btn btn-default" })</p>
        <p>Create a new account</p>
    </div>
</div>


